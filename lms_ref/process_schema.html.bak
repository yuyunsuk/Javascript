function detailShow(obj){
    tN = $(obj).closest("tr");
    if(tN.hasClass("active")){
        tN.removeClass("active");
        tN.nextUntil(".dep1").hide();
    }else{
        tN.addClass("active");
        tN.nextUntil(".dep1").show();
    }
}

function mobDetailShow(obj){
    tN = $(obj).closest(".order");
    if(tN.hasClass("active")){
        tN.removeClass("active");
        tN.siblings('ul').hide();
    }else{
        tN.addClass("active");
        tN.siblings('ul').show();
    }
}

/* 전체 열기 닫기 스크립트 */
function allToggle(obj){
    tN = jQuery(obj);
    pcList = jQuery(obj).closest(".btnBoxR").siblings('.tableScroll');
    mobList = jQuery(obj).closest(".btnBoxR").siblings(".attend_list");

    if(tN.hasClass("active")){
        jQuery(obj).find('span').text('전체열기');
        jQuery(obj).removeClass('active');

        pcList.removeClass("active");
        pcList.find('tbody tr.dep1').removeClass('active');
        pcList.find('tbody tr.dep2').hide();

        mobList.removeClass("active");
        mobList.find('.order').removeClass("active");
        mobList.find('ul').hide();

    }else{

        jQuery(obj).find('span').text('전체닫기');
        jQuery(obj).addClass('active');

        pcList.addClass("active");
        pcList.find('tbody tr.dep1').addClass('active');
        pcList.find('tbody tr.dep2').show();

        mobList.addClass("active");
        mobList.find('.order').addClass("active");
        mobList.find('ul').show();

    }
}

