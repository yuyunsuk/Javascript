function chkChasiViewerEduyn(id,charsino,status){
	var serverNow = new Date(1718873363816);
 	var pcNow = new Date();
 	var startDay = new Date(2018, 6, 02, 00, 00, 00);
 	var endDay = new Date(2018, 6, 02, 23, 59, 00);

 	var distinction = pcNow.getTime() - loadPcNow.getTime();

	jQuery.ajax({
		url : getChasiEduynUrl,
		cache : false,
		dataType: 'json',
		type: 'POST',
		async:false,
		data : {
			p_year : jQuery("#p_year").val(),
			p_contsid : jQuery("#p_contsid").val(),
			p_crscd : jQuery("#p_crscd").val(),
