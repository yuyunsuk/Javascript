			p_itemid : id,
			p_selitem : id,
			p_empinsrnc_aply_yn: jQuery("#p_empinsrnc_aply_yn").val()
		},
		success : function(data){
			var p_empinsrnc_check = jQuery("#p_empinsrnc_aply_yn").val();

			if(data.p_frmatn_exam_yn == "Y" && '195' > 0){
				location.href= "wbviewer://studyClose?msg=" + "이전 차시에 제출된 형성평가를 완료하셔야 학습이 가능합니다.";
                return;
            }

			if(data.p_off_time_yn == "Y" && data.p_complteyn != "Y" && '195' > 0){
                location.href= "wbviewer://studyClose?msg=" + "병행 학습으로 설정된 집체훈련 과정의 학습시간입니다.";
                return;
            }

            if(status == "N" && data.p_pre_complteyn == "N" && jQuery("#p_stdy_prgrs_mthd_cd").val() == 'S' && '195' > 0){
                location.href = "wbviewer://sendMsg?type=P&msg=" + "학습을 완료하셔야 다음페이지로 이동이 가능합니다.";
                return;
            }

			if(data.p_studytimePassYn == "N" && p_empinsrnc_check != "N" && '195' > 0) {
				var studytimePassMsg = "고용보험 환급 과정은 차시 단위 " + data.p_min_studytime + "분 이상 학습을 진행하셔야 다음 차시 학습이 가능합니다.";
				var s_application_connect = "";

                location.href= "wbviewer://studyClose?msg=" + studytimePassMsg;
				return;
			}

			//2017.08.24 13:00 ~ 2017.08.24 19:00까지 캡챠 기능 일시 중지
			if(data.p_eduyn == "Y") {
				if(((serverNow.getTime()+distinction) > startDay.getTime()) && (serverNow.getTime()+distinction) < endDay.getTime()){
					jsStudyStartPopUp(id);
                }else{
                    if('N' == 'Y' && 'N' == 'N' &&  jQuery("#p_stdy_prgrs_mthd_cd").val() == 'S'){
                        if( ((9-charsino) % 8) == 0 ){
                            //(공통)iOS OTP 인증오류 보완-210909
                            if("Y" == data.p_complteyn){
                                jsStudyStartPopUp(id);
                            }else{
                                if(data.p_chapcha == "Y" && p_empinsrnc_check != "N" && 'N' == "N"){
                                    jsStudyStartPopUp(id);
                                }else{
                                    if(navigator.userAgent.indexOf("APP_Running")>-1){
                                        location.href = "wbviewer://studyClose?msg=" + "해당 목차는 인증이 필요합니다.";
                                    }
                                    //alert("해당 목차는 보안인증이 필요합니다.");
                                    //parent.jsPrgrsCaptcha(nextNode.code, nextNode.chasiid);
                                }
                            }
                        }else{
                            jsStudyStartPopUp(id);
                        }
                    }else{
                        if(jQuery("#p_stdy_prgrs_mthd_cd").val() == 'S'){
                            jsStudyStartPopUp(id);
                        }else{
                            jsStudyStartPopUp(id);
                        }
                    }
                }
			} else {
                if('N' == 'Y' && jQuery("#p_empinsrnc_aply_yn").val() == "Y"){
                    var dayLmtRate = Number(data.p_day_progrs_lmt_rate); //0
                    var crsDayLmtRate = Number(data.p_crs_day_progrs_lmt_rate); //0

                    if(crsDayLmtRate <= dayLmtRate){
                    	var dayLmtRateMsg = "해당과정의 일일학습분량을 초과하였습니다.";
                        dayLmtRateMsg += "\n(" + "해당 학습과정 일일학습제한 " + crsDayLmtRate + "시간(차시), 일일학습완료 " + crsDayLmtRate + "시간(차시)로 제한" + ")";

                    	location.href = "wbviewer://studyClose?msg=" + dayLmtRateMsg;
                    }else{
                        location.href = "wbviewer://studyClose?msg=" + "해당과정의 일일학습분량을 초과하였습니다.\n집체 및 원격 각 8시간, 합산 8시간(차시) 제한"
                    }
                }else{
                    location.href = "wbviewer://studyClose?msg=" + "해당과정의 일일학습분량을 초과하였습니다.";
                }
			}
		},
		error : function(result){
		}
	});
}

