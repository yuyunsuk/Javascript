function doSurveyCheck(){
    jQuery.ajax({
		url : '/usrs/lms/classrm/classSurveyList.do',
		dataType : 'json',
		type : 'POST',
		cache : false,
		async : false,
		data : {
			p_pageno : 1,
			p_listscale : 100,
			p_sortorder : "SURVEY_ID DESC",
			p_crsseq_id : jQuery("#p_crsseq_id").val(),
			mkey : ''
		},
		success : function(result){
			if(result != null){
				if(result.surveyList.length > 0){
					for(var i = 0; i < result.surveyList.length; i++){
						if(result.surveyList[i].SURVEY_KND_CD == "02" &&
							(result.surveyList[i].SBMT_STATE_CD == "" || result.surveyList[i].SBMT_STATE_CD == "T" || result.surveyList[i].SBMT_STATE_CD == undefined) &&
							result.surveyList[i].SURVEY_PERIOD == "PRGRS"){
							doSurveyWrite(result.surveyList[i].SURVEY_ID, result.surveyList[i].SURVEY_PAPER_ID, result.surveyList[i].SURVEY_KND_CD, result.surveyList[i].STUDENT_ID);
							break;
						}
					}
				}
			}

			pageDelayReload();
		}
	});
}

